<script src="$utilWebconfig.getStaticResourcesPath()/resources/js/layerModal.js" type="text/javascript"></script>

	<form class="form-signin" id="themeListForm" name="themeListForm" action="$!utilWebconfig.getActionDefaultFontPath()/B2C/themeManage/list" method="GET" role="form">

        <div class="tableGroup whole">
			
            <table class="table table-hover">
                    <tr>
                       <th><input type="checkbox" name="checkAll" id="checkAll" value="" /> 全选</th>
						<th>图片</th>
                        <th>景点名称</th>
                        <th>操作</th>
                    </tr>
                    #foreach($scenic in $scenicDOList.itemList)
                    <tr class="subCheckedTR">
                        <td><input class="subChecked" type="checkbox" value="$!scenic.id"></td>
						<td><img src="$!scenic.url"></td>
                        <td>$!scenic.name</td>
						<td>#*<button type="button" class="btn btn-primary scenicDetail" scenicId="$scenic.id">查看</button>*#</td>
                    </tr>
                    #end
            </table>
        </div>
    #*分页
    #parse("/page.vm")
    #pageStyle("themeListForm")
	*#
	<div class="clubBotBtn">
            <div class="btn-group">
                <button class="btn btn-success" type="button" id="btnSubmitSen" >添加</button>
            </div>
            <div class="btn-group">
                <button class="btn">取消</button>
            </div>
   </div>
    </form>

<script>
	$('#btnSubmitSen').click(function(){
 		openModalForForm(actionDefaultPath + '/B2C/resourceForSelect/selectOneScenic','选择景区',function() {
			var sce = win.getItems();
			$("#name").val(sce.name);
			$("#outId").val(sce.id);
			$("#picUrls").val(sce.logoUrl);
			return true;
		});
	})
</script>