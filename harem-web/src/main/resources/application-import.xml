<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
    xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-4.0.xsd
       http://code.alibabatech.com/schema/dubbo http://code.alibabatech.com/schema/dubbo/dubbo.xsd"
    >

    <bean id="userDubboApplicationConfig" class="com.alibaba.dubbo.config.ApplicationConfig">
        <property name="name" value="${app.name.user}"/>
    </bean>

    <bean id="consumerConfig" class="com.alibaba.dubbo.config.ConsumerConfig">
        <property name="check" value="${dubbo.import.consumer.user.check}"/>
    </bean>

    <bean id="importUserService" class="com.alibaba.dubbo.config.spring.ReferenceBean">
        <property name="interface" value="com.yimayhd.user.client.service.UserService"/>
        <property name="application" ref="userDubboApplicationConfig"/>
        <property name="registry" ref="dubboRegistryConfig"/>
        <property name="consumer" ref="consumerConfig"/>
        <property name="version" value="${dubbo.import.user.version}"/>
        <property name="check" value="${dubbo.import.consumer.user.check}"/>
    </bean>
</beans>