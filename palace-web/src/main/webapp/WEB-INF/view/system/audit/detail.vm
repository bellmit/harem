<div class="topicbox">
	<form class="form-signin" id="topicListForm" name="topicListForm" action="$!utilWebconfig.getActionDefaultFontPath()/jiuxiu/topic/list" method="GET" role="form">
		
		<div class="inputGroup whole">
			<div class="input-group inpSingle">
				<span class="input-group-btn">
                    <button class="btn btn-default" type="button">对账日期</button>
                </span>
				<input type="text" class="form-control form-date dateInp" readonly name="" value="">
			</div>
			<div class="input-group inpSingle ">
				<span class="input-group-btn">
                    <button class="btn btn-default" type="button">对账渠道</button>
                </span>
				<select data-placeholder="请选择..." class="chosen-select form-control" tabindex="2" name="hasContent">
                    <option value="">全部</option>
                    <option value="1" >支付宝</option>
                    <option value="2" >微信支付</option>
                    <option value="3" >光大云支付</option>
                    <option value="4" >光大电子钱包</option>
                </select>
			</div>
			<div class="input-group inpSingle ">
				<span class="input-group-btn">
                    <button class="btn btn-default" type="button">对账结果</button>
                </span>
				<select data-placeholder="请选择..." class="chosen-select form-control" tabindex="2" name="hasContent">
                    <option value="">全部</option>
                    <option value="1" >金额不一致</option>
                    <option value="2" >状态不一致</option>
                    <option value="3" >他有我无</option>
                    <option value="4" >我有他无</option>
                    <option value="5" >对账相符</option>
                </select>
			</div>
			<div class="btn-group">
				<button type="submit" class="btn btn-primary search">查询</button>
			</div>
			<div class="btn-group">
				<button type="button" class="btn btn-primary reset">重置</button>
			</div>
		</div>
		
		<div class="tableGroup whole">
			<table class="table table-hover topicTable">
				<tbody>
					<tr>
						<th>交易订单号</th>
						<th>交易时间</th>
						<th>渠道类型</th>
						<th>交易类型</th>
						<th>支付订单号</th>
						<th>渠道订单号</th>
						<th>平台支付金额</th>
						<th>渠道支付金额</th>
						<th>平台状态</th>
						<th>渠道状态</th>
						<th>对账状态</th>
					</tr>
					<tr>
						<td>jy123345</td>
						<td>2016-7-7 20:13:02</td>
						<td>微信支付</td>
						<td>消费</td>
						<td>zf9876</td>
						<td>zf9876</td>
						<td>300.00</td>
						<td>288.00</td>
						<td>成功</td>
						<td>成功</td>
						<td>金额不一致</td>
				</tbody>
			</table>
		</div>
		#parse("/page.vm")
		#pageStyle("topicListForm")
	</form>
</div>
<script type="text/javascript" src="$rs_js_css_path_jiuxiu/js/plugin/datetimepicker.min.js" ></script>
<script type="text/javascript">
	
	(function(){
	
		$(".form-date").datetimepicker({
            language:  "zh-CN",
            weekStart: 1,
            todayBtn:  1,
            autoclose: 1,
            todayHighlight: 1,
            startView: 2,
            minView: 2,
            forceParse: 0,
            format: "yyyy-mm-dd"
        });
        
	})();
	
	
</script>
