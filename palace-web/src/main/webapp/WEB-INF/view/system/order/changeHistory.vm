#set($layout = "/system/layout/layout_non.vm")
<!-- head start-->
<!DOCTYPE html>
<html lang="zh_CN">
<head>
<title>修改历史</title>
<link rel="stylesheet" href="$rs_js_css_path_jiuxiu/css/zui.min.css">
<link rel="stylesheet" href="$rs_js_css_path_jiuxiu/css/changeOrderStatus.css">
</head>
<!-- head end-->
<body>
	#parse("/system/layout/layout-jiuxiu-meta.vm")
	<div class="search-box">
		<form id="frmChangeHistory" name="frmChangeHistory">
			<div class="input-group">
				<span class="input-group-btn">
							<button class="btn btn-default" type="button">订单编号：</button>
						</span>
				<input type="text" class="form-control" name="orderNo" value="">
			</div>
			<div class="input-group">
				<span class="input-group-btn">
							<button class="btn btn-default" type="button">修改人ID：</button>
						</span>
				<input type="text" class="form-control" name="orderNo" value="">
			</div>
			<div class="input-group">
				<span class="input-group-btn">
		                    <button class="btn btn-default" type="button">修改时间</button>
		                </span>
				<input type="text" class='form-control form-date date start-time' placeholder='' readonly name="changeTimeStart" value="">
				<span class="input-group-addon fix-border fix-padding date-addon">至</span>
				<input type="text" class='form-control form-date date end-time' placeholder='' readonly name="changeTimeEnd" value="">
			</div>
			<div class="btn-group">
				<button type="submit" class="btn btn-primary">查询</button>
			</div>
			<div class="btn-group">
				<button type="button" class="btn btn-primary" id="btnReset">重置</button>
			</div>
		</form>
	</div>
	<table class="table table-hover table-bordered">
		<thead>
			<tr>
				<th>
					订单编号
				</th>
				<th>
					修改人ID
				</th>
				<th>
					修改日期
				</th>
				<th>
					修改内容
				</th>
				<th>
					原因备注
				</th>
			</tr>
		</thead>
		<tbody>
			<tr>

				<td>
					abc123123
				</td>
				<td>
					2u11010101
				</td>
				<td>
					2016-10-08 16:00
				</td>
				<td>
					<div class="t-c">
						订单状态
					</div>
					<div class="t-c">
						已付款--取消
					</div>
				</td>
				<td style="width:40%">
					买家退货
				</td>
			</tr>
		</tbody>
	</table>

	<script type="text/javascript" src="$rs_js_css_path_jiuxiu/js/sea.js"></script>
	<script type="text/javascript" src="$rs_js_css_path_jiuxiu/js/seajs-preload.js"></script>
	<script type="text/javascript" src="$rs_js_css_path_jiuxiu/js/config.js"></script>
	<script type="text/javascript">
		seajs.use('orderChangeHistory');
	</script>
</body>
</html>

