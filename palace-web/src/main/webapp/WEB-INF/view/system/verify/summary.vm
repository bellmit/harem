<div class="topicbox">
	<form class="form-signin" id="topicListForm" name="topicListForm" action="$!utilWebconfig.getActionDefaultFontPath()/jiuxiu/topic/list" method="GET" role="form">
		
		<div class="inputGroup whole">
			<div class="input-group inpSingle">
				<span class="input-group-btn">
                    <button class="btn btn-default" type="button">对账日期</button>
                </span>
				<input type="text" class="form-control form-date dateInp" readonly name="" value="">
			</div>
			<div class="input-group inpSingle ">
				<span class="input-group-btn">
                    <button class="btn btn-default" type="button">对账渠道</button>
                </span>
				<select data-placeholder="请选择..." class="chosen-select form-control" tabindex="2" name="hasContent">
                    <option value="">全部</option>
                    <option value="" >微信支付</option>
                    <option value="" >支付宝</option>
                    <option value="" >光大云支付</option>
                    <option value="" >光大电子钱包</option>
                </select>
			</div>
			<div class="input-group inpSingle ">
				<span class="input-group-btn">
                    <button class="btn btn-default" type="button">对账结果</button>
                </span>
				<select data-placeholder="请选择..." class="chosen-select form-control" tabindex="2" name="hasContent">
                    <option value="">全部</option>
                    <option value="" >对账相符</option>
                    <option value="" >对账不符</option>
                </select>
			</div>
			<div class="btn-group">
				<button type="submit" class="btn btn-primary search">查询</button>
			</div>
			<div class="btn-group">
				<button type="button" class="btn btn-primary reset">重置</button>
			</div>
		</div>
		
		<div class="tableGroup whole">
			<table class="table table-hover topicTable">
				<tbody>
					<tr>
						<th>对账时间</th>
						<th>对账渠道</th>
						<th>平台支付金额</th>
						<th>银行支付金额</th>
						<th>平台支付笔数</th>
						<th>银行支付笔数</th>
						<th>差异笔数</th>
						<th>差异金额</th>
						<th>对账结果</th>
						<th>操作</th>
					</tr>
					<tr>
						<td>2016-04-07 10：00：00</td>
						<td>光大电子账户</td>
						<td>51000.00</td>
						<td>50000.00</td>
						<td>230</td>
						<td>231</td>
						<td>1</td>
						<td>1000.00</td>
						<td>对账不符</td>
						<td>
							<button type="button" class="btn btn-primary detail">下载对账单</button>
						</td>
				</tbody>
			</table>
		</div>
		#parse("/page.vm")
		#pageStyle("topicListForm")
	</form>
</div>
<script type="text/javascript">
	
</script>
