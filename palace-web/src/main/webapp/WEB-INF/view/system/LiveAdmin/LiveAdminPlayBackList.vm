<body style="">
<div class="dialog">
    <div class="bgmeng"></div>
    <div class="dlgcont">
        <table>
            <tbody><tr>
                <td class="tit">权重：</td>
                <td>
                    <input type="text" id="priority">
                    <input type="hidden" id="rowId" value="">
                    <input type="hidden" id="oldPriority" value="">
                </td>
            </tr>
            </tbody></table>
        <div class="boxbtn">
            <button class="ok">确定</button>
            <button class="cancel">取消</button>
        </div>
    </div>
</div>

<div class="searchbox">
    <form id="fm">
        <table>
            <tbody><tr>
                <td class="tit">达人昵称：</td>
                <td><input name="nickname" maxlength="30"></td>
                <td class="tit">达人ID：</td>
                <td><input name="talentID" maxlength="30"></td>
                <td class="tit">直播分类：</td>
                <td><select name="liveCategory">
                    <option value="0">0</option>
                    <option value="1">1</option>
                    <option value="2">2</option>
                </select></td>
                <td class="tit">状态：</td>
                <td><select name="status">
                    <option value="onSale">上架</option>
                    <option value="offSale">下架</option>
                    <option value="deleted">用户已删除</option>
                </select></td>
            </tr>
            <tr>
                <td class="tit">开始时间：</td>
                <td colspan="3">
                    <input type="text" name="startTime" value="2016-04-21" class="form-date dateInp startTime inputxt">
                    &nbsp; &nbsp;至&nbsp; &nbsp;
                    <input type="text" name="endTime" value="2016-04-30" class="form-date dateInp endTime inputxt">
                </td>
                <td class="tit">排序方式：</td>
                <td><select name="orderStyle">
                    <option value="0">时间倒序(最新回放在前)</option>
                    <option value="1">权重(权重值高在前)</option>
                    <option value="2">播放次数(次数多在前)</option>
                </select></td>
                <td colspan="2">
                    <button class="searchbtn">查询</button>
                    <button class="resetbtn">重置</button>
                </td>
            </tr>
            </tbody></table>
    </form>
</div>

<div class="gridbox">
    <table class="tbgrid">
        <thead>
        <tr><th>ID</th>
            <th>头图</th>
            <th>直播标题</th>
            <th>直播分类</th>

            <th>话题</th>
            <th>达人昵称</th>
            <th>达人ID</th>

            <th>开始时间</th>
            <th>结束时间</th>
            <th>时长</th>
            <th>观看次数</th>

            <th>权重</th>
            <th>状态</th>
            <th>操作</th>

        </tr></thead>
        <tbody>
        <tr id="row1">
            <td class="rowId">12309</td>
            <td><img src="http://img0.imgtn.bdimg.com/it/u=3691748163,484693479&amp;fm=206&amp;gp=0.jpg" alt=""></td>
            <td>标题测试</td>
            <td>分类测试</td>
            <td>话题测试</td>
            <td>达人昵称测试</td>
            <td>达人id测试</td>
            <td>2016-02-15 15:00</td>
            <td>2016-02-15 16:00</td>
            <td>1h</td>
            <td>15</td>
            <td class="priorityValue">45</td>
            <td>上架</td>
            <td>
                <label class="exam dialogBtn">编辑权重</label>
                <label class="exam onSaleBtn">上架</label>
                <label class="exam offSaleBtn">下架</label>
                <label class="exam playBtn">播放</label>
            </td>
        </tr>
        </tbody>
    </table>

    <div class="pagebox">
        <div class="actionpage"><button class="prev">上一页</button><label>当前第 </label><label class="curpage">1</label><label> 页</label><button class="next">下一页</button></div>
        <div class="pageinfo"><label>共：</label><label class="totalPage">20</label><label>页，</label>
            <label class="totalCount">1000</label><label>条</label></div>
    </div>

</div>

